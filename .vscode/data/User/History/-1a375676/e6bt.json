{
  "_version": "1.65.0",
  "sap.app": {
    "id": "assessment1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.2",
      "toolsId": "c6150ddc-23a9-42e7-8dad-2711da0a867a"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.138.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "assessment1.i18n.i18n"
        }
      },
      "mat":{
        "type" : "sap.ui.model.json.JSONModel",
        "uri" : "model/products.json"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "assessment1.view",
        "async": true,
        "viewPath": "assessment1.view"
      },
      "routes": [
        {
          "name": "RouteAssessment1",
          "pattern": ":?query:",
          "target": [
            "TargetAssessment1"
          ]
        },
        {
         "name":"productDetailPage",
          "pattern":"productDetailPage/{productIndex}",
          "target": ["secondView"]
        },
        {
          "name":"vendorDetailPage",
           "pattern":"vendorDetailPage/{productIndex}",
           "target": ["secondView"]
         },
      ],
      "targets": {
        "TargetAssessment1": {
          "id": "Assessment1",
          "name": "Assessment1"
        },
        "secondView":{
          "id" : "productDetail",
          "name" : "productDetailPage"
        }
      }
    },
    "rootView": {
      "viewName": "assessment1.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
